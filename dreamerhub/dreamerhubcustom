-- Scripts:

local function KODTPM_fake_script() -- LoadstringExecute.LocalScript 
	local script = Instance.new('LocalScript', LoadstringExecute)

	local textButton = script.Parent
	local textBox = script.Parent:FindFirstChild("BoxExecute")
	
	if not textBox then
		warn("TextBox 'BoxExecute' not found in the parent!")
		return
	end
	
	local function executeLoadstringOrLua()
		local input = textBox.Text
	
		if input and input ~= "" then
			local success, funcOrErr = pcall(function()
				return loadstring(input)
			end)
	
			if success and funcOrErr then
				local execSuccess, execErr = pcall(funcOrErr)
	
				if not execSuccess then
					warn("Error executing Lua code: " .. execErr)
				end
			else
				warn("Error compiling Lua code: " .. funcOrErr)
			end
	
			script.Parent.Parent:Destroy()
		else
			warn("No input provided in the TextBox.")
		end
	end
	
	textButton.MouseButton1Click:Connect(executeLoadstringOrLua)
	
end
coroutine.wrap(KODTPM_fake_script)()
